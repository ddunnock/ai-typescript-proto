{
  "name": "@repo/agents",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": { "types": "./dist/index.d.ts", "import": "./dist/index.js" },
    "./base": { "types": "./dist/base.d.ts", "import": "./dist/base.js" },
    "./email": { "types": "./dist/email-agent.d.ts", "import": "./dist/email-agent.js" },
    "./notes": { "types": "./dist/notes-agent.d.ts", "import": "./dist/notes-agent.js" },
    "./document": { "types": "./dist/document-agent.d.ts", "import": "./dist/document-agent.js" },
    "./research": { "types": "./dist/research-agent.d.ts", "import": "./dist/research-agent.js" },
    "./orchestrator": { "types": "./dist/orchestrator.d.ts", "import": "./dist/orchestrator.js" }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@repo/shared": "workspace:*",
    "@repo/providers": "workspace:*",
    "@repo/mcp": "workspace:*"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0"
  }
}