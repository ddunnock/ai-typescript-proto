{
  "name": "@repo/agents",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": { "types": "./dist/index.d.ts", "import": "./dist/index.js" },
    "./base": { "types": "./dist/base.d.ts", "import": "./dist/base.js" },
    "./state": { "types": "./dist/state.d.ts", "import": "./dist/state.js" },
    "./specialized": { "types": "./dist/specialized/index.d.ts", "import": "./dist/specialized/index.js" }
  },
  "scripts": {
